#include <stdio.h>

/**
 * main - Entry point
 *
 * Description: Program that prints _putchar
 *
 * Return: 0 (Success)
 */
int main(void)
{
	 int ch;

	 ch = '_';
	 putchar(ch);
	 ch = 'p';
	 putchar(ch);
	 ch = 'u';
	 putchar(ch);
	 ch = 't';
	 putchar(ch);
	 ch = 'c';
	 putchar(ch);
	 ch = 'h';
	 putchar(ch);
	 ch = 'a';
	 putchar(ch);
	 ch = 'r';
	 putchar(ch);
	 putchar('\n');
	 return (0);
}
